{
  "name": "ar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build:storybook": "build-storybook",
    "dev:storybook": "start-storybook -p 3006 -c .storybook -s ./public",
    "test": "npm run test:jest && npm run test:eslint",
    "test:jest": "jest ./src",
    "test:jest:watch": "npm run test:jest -- --watch",
    "test:eslint": "eslint ./src",
    "test:flow": "flow",
    "report": "cat ./coverage/lcov.info | coveralls"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@atomix/eslint-config": "^6.4.0",
    "@atomix/eslint-config-react": "^6.4.0",
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-knobs": "^3.4.1",
    "@storybook/addon-storysource": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/cli": "^3.4.1",
    "@storybook/react": "^3.4.1",
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.10.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.69.0",
    "happypack": "^4.0.1",
    "highlight-loader": "^0.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "markdown-loader": "^2.0.2",
    "react-hot-loader": "^3.1.3",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.3.1",
    "rimraf": "^2.6.2",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-readme": "^3.3.0",
    "style-loader": "^0.20.3",
    "svg-react-loader": "^0.4.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "color": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "grid-styled": "^3.2.1",
    "highlight.js": "^9.12.0",
    "intl": "^1.2.5",
    "lodash": "^4.17.5",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.3.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.2.5",
    "styled-normalize": "^4.0.0",
    "styled-theme": "^0.3.3",
    "styled-theming": "^2.2.0",
    "styled-tools": "^0.5.0",
    "validator": "^13.7.0",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "browser": true,
    "setupTestFrameworkScriptFile": "<rootDir>/config/setup-jest-tests.js",
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/ui/themes/",
      "(.test)\\.(ts|tsx|js)$",
      "(.story)\\.(ts|tsx|js)$"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
